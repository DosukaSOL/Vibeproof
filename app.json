{
  "expo": {
    "name": "VibeProof",
    "slug": "vibeproof",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vibeproof",
    "icon": "./assets/vpicon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "owner": "dosukasol",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dosukasol.vibeproof",
      "associatedDomains": [
        "applinks:*.solana-mobile.com",
        "applinks:*.identity.solana"
      ]
    },

    "android": {
      "package": "com.dosukasol.vibeproof",
      "adaptiveIcon": {
        "foregroundImage": "./assets/vpicon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            {
              "scheme": "vibeproof"
            }
          ]
        },
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            {
              "scheme": "https",
              "host": "*.solana-mobile.com",
              "pathPrefix": ""
            }
          ]
        }
      ]
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f13fa36d-6d7d-4ca2-b3f8-c64e1f387593"
      }
    },

    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    }
  }
}
