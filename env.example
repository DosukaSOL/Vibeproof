# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=eyJhbGc...your_anon_key_here

# Solana Cluster
EXPO_PUBLIC_SOLANA_CLUSTER=mainnet-beta

# X (Twitter) OAuth 2.0 — get from developer.x.com
EXPO_PUBLIC_X_CLIENT_ID=your_x_client_id_here

# GitHub OAuth — get from github.com/settings/developers
EXPO_PUBLIC_GITHUB_CLIENT_ID=your_github_client_id_here

# Optional: Enable debug logging
EXPO_DEBUG=false
